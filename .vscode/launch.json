    "version": "0.2.0",
    "configurations": [

        {
            "name": "launch app",
            "type": "debugpy",
            "request": "launch",
            "program": "./ui/app.py",
            "console": "integratedTerminal",
            "workingDirectory": "${workspaceFolder}/special_agent"
